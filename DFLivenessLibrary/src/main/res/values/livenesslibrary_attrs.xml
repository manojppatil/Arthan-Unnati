<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleTimeView">
        <attr name="circle_width" format="integer" />
        <attr name="circle_color" format="string" />
        <attr name="max_time" format="integer"></attr>
        <attr name="text_size" format="integer"></attr>
        <attr name="text_color" format="string"></attr>
        <attr name="redus_color" format="string"></attr>
        <attr name="text_redus" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

</resources>